# =============================================================================
# ODG Frontend - Configuration Environment Variables
# =============================================================================
# Copiez ce fichier vers .env pour la configuration locale

# -----------------------------------------------------------------------------
# API Configuration
# -----------------------------------------------------------------------------
# URL du backend API (sans trailing slash)
VITE_API_BASE_URL=http://localhost:5000

# -----------------------------------------------------------------------------
# Application Configuration
# -----------------------------------------------------------------------------
# Nom de l'application
VITE_APP_NAME=ODG WebGIS

# Environnement (development, staging, production)
VITE_APP_ENV=development

# -----------------------------------------------------------------------------
# Features Flags (optionnel)
# -----------------------------------------------------------------------------
# Activer le module blockchain
VITE_FEATURE_BLOCKCHAIN=true

# Activer les outils de debug
VITE_DEBUG_MODE=false
